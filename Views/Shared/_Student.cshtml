@model IEnumerable<AjaxWithAsp.netMvc.Models.Student>

@{
    ViewBag.Title = "_Student";
}

<h2>Student</h2>
<table style ="border: 1px solid black;background-color: silver"class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TotalMarks)
        </th>
        
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TotalMarks)
        </td>
    </tr>
}

</table>
